	"version": "0.2.0",
	"configurations": [
		{
			"name": "promise",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/build/debug/mujs",
			"args": ["${workspaceFolder}/note/demo/promise.js"],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
					{
							"description": "Enable pretty-printing for gdb",
							"text": "-enable-pretty-printing",
							"ignoreFailures": true
					},
					{
							"description": "Set Disassembly Flavor to Intel",
							"text": "-gdb-set disassembly-flavor intel",
							"ignoreFailures": true
					}
			],
			// "preLaunchTask": "C/C++: gcc build active file",
			"miDebuggerPath": "/usr/bin/gdb"
		},
		{
			"name": "curl",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/note/demo/build/curl_libevent",
			"args": ["https://jsonplaceholder.typicode.com/todos/1", "https://jsonplaceholder.typicode.com/todos/2"],
			"stopAtEntry": true,
			"cwd": "${fileDirname}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
					{
							"description": "Enable pretty-printing for gdb",
							"text": "-enable-pretty-printing",
							"ignoreFailures": true
					},
					{
							"description": "Set Disassembly Flavor to Intel",
							"text": "-gdb-set disassembly-flavor intel",
							"ignoreFailures": true
					}
			],
			// "preLaunchTask": "C/C++: gcc build active file",
			"miDebuggerPath": "/usr/bin/gdb"
		},
		{
			"name": "libuv",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/note/demo/libuv/uv_simple",
			"args": [],
			"stopAtEntry": true,
			"cwd": "${fileDirname}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
					{
							"description": "Enable pretty-printing for gdb",
							"text": "-enable-pretty-printing",
							"ignoreFailures": true
					},
					{
							"description": "Set Disassembly Flavor to Intel",
							"text": "-gdb-set disassembly-flavor intel",
							"ignoreFailures": true
					}
			],
			// "preLaunchTask": "C/C++: gcc build active file",
			"miDebuggerPath": "/usr/bin/gdb"
		}
	]
}